{
    "extends": "airbnb",
    "env": {
      "browser": true
    },
    "rules": {
      "semi": [2, "always"],
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "jsx-a11y/anchor-is-valid": [ "error", {
        "components": [ "Link" ],
        "specialLink": [ "to" ]
      }],
      "no-underscore-dangle": 0,
      "linebreak-style": 0
    }
  }